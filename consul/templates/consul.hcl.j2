datacenter = "{{ consul_datacenter }}"
data_dir = "{{ consul_data_dir }}"
encrypt = "{{ consul_gossip_encryption_key }}"
ca_file = "/etc/consul.d/{{ consul_ca_file }}"
cert_file = "/etc/consul.d/{{ consul_cert_file }}"
key_file = "/etc/consul.d/{{ consul_key_file }}"
verify_incoming = {{ consul_verify_incoming }}
verify_outgoing = {{ consul_verify_outgoing }}
verify_server_hostname = {{ consul_verify_server_hostname }}
retry_join = ["{{ consul_retry_join_address }}"]

acl = {
  enabled = true
  default_policy = "deny"
  enable_token_persistence = true
}

performance {
  raft_multiplier = 1
}
